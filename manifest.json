{
  "manifest_version": 2,
  "name": "vk-tenor",
  "description": "Adds Tenor gif search in VK messenger",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "permissions": [
    "*://g.tenor.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "tenor.html"
  ]
}